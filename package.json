{
  "name": "prismic-javascript",
  "description": "JavaScript development kit for prismic.io",
  "license": "Apache-2.0",
  "url": "https://github.com/prismicio/prismic-javascript",
  "keywords": [
    "prismic",
    "prismic.io",
    "cms",
    "content",
    "api"
  ],
  "version": "2.1.5",
  "devDependencies": {
    "@types/node-fetch": "^2.5.5",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^4.0.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nock": "^11.6.0",
    "node-fetch": "^2.6.0",
    "rollup": "^2.1.0",
    "rollup-plugin-terser": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typedoc": "^0.16.10",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prismicio/prismic-javascript.git"
  },
  "main": "dist/prismic-javascript.min.js",
  "module": "dist/prismic-javascript.min.mjs",
  "browser": {
    "./dist/prismic-javascript.js": "dist/prismic-javascript.browser.min.js",
    "./dist/prismic-javascript.mjs": "dist/prismic-javascript.browser.min.mjs"
  },
  "files": [
    "dist/*.js"
  ],
  "types": "./d.ts/index.d.ts",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "check": "tsc --pretty --noEmit --watch",
    "test": "mocha",
    "prepublish": "npm run build; npm run docs; npm run declarations",
    "docs": "typedoc --mode file --out ./docs ./src/index.ts",
    "declarations": "tsc --emitDeclarationOnly --declaration --declarationDir ./d.ts",
    "lint": "tslint src/**/*.ts -e src/lru.ts -e src/Cookies.ts"
  }
}
