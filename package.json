{
  "name": "prismic-javascript",
  "description": "JavaScript development kit for prismic.io",
  "license": "Apache-2.0",
  "url": "https://github.com/prismicio/prismic-javascript",
  "keywords": [
    "prismic",
    "prismic.io",
    "cms",
    "content",
    "api"
  ],
  "version": "3.0.0-beta.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/node-fetch": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "mocha": "^5.2.0",
    "nock": "^11.6.0",
    "node-fetch": "^2.6.0",
    "rollup": "^2.1.0",
    "rollup-plugin-terser": "^5.3.0",
    "tslib": "^1.11.1",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prismicio/prismic-javascript.git"
  },
  "main": "dist/prismic-javascript.min.js",
  "module": "dist/prismic-javascript.min.mjs",
  "browser": "dist/prismic-javascript.browser.min.js",
  "files": [
    "dist/*.js"
  ],
  "types": "./d.ts/index.d.ts",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "check": "tsc --pretty --noEmit --watch",
    "test": "npm run build && mocha",
    "prepublish": "npm run build; npm run docs; npm run declarations",
    "docs": "typedoc --mode file --out ./docs ./src/index.ts",
    "declarations": "tsc --emitDeclarationOnly --declaration --declarationDir ./d.ts",
    "lint": "eslint src/**/*.ts"
  }
}
